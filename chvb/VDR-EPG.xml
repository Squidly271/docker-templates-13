<?xml version="1.0" encoding="utf-8"?>
<Container>
<Beta>False</Beta>
<Category>Network:DVB</Category>
<Date>2016-06-01</Date>
  <Name>VDR-EPG</Name>
  <Support>-</Support>
  <Overview>This is the EPG Database for the VDR. VDR is a DVB Server Software</Overview>
  <Changes>[font size=4]VDR EPG Database[/font][/center]
[b]01.06.2016:[/b] Initial Release</Changes>
  <Description>This is the EPG Database for the VDR. VDR is a DVB Server Software, but running on Linux.[br]
  [b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
  [b]Port 39876:[/b] Administrative Configuration [br]
  [b]Port 2222:[/b] SSH Server Port Login with User:docker / Passwort:test123 [br]
  </Description>
  <Project>http://tvdr.de/</Project>
  <Registry>https://hub.docker.com/r/chriszero/vdr-epgd/</Registry>
  <Repository>chriszero/vdr-epgd</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
        <Port>
        <HostPort>9999</HostPort>
        <ContainerPort>9999</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/VDR/epgd/</HostDir>
      <ContainerDir>/epgd/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>1.0</Version>
  <WebUI>https://[IP]:[PORT:8001]/</WebUI>
  <Banner>https://raw.githubusercontent.com/chvb/docker-templates/master/chvb/img/VDR.jpg</Banner>
  <Icon>https://raw.githubusercontent.com/chvb/docker-templates/master/chvb/img/VDR.jpg</Icon>
  <ExtraParams></ExtraParams>
</Container>
